<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="165.0" prefWidth="500.0"
  style="-fx-background-color: #A67244;" xmlns="http://javafx.com/javafx/19"
  xmlns:fx="http://javafx.com/fxml/1">
  <AnchorPane layoutX="5.0" layoutY="7.0" prefHeight="151.0" prefWidth="489.0"
    style="-fx-background-color: #F2E5D5;" AnchorPane.bottomAnchor="7.0" AnchorPane.leftAnchor="5.0"
    AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="7.0">
    <Button fx:id="cancelButton" layoutX="285.0" layoutY="98.0" mnemonicParsing="false"
      onAction="#cancel" prefHeight="25.0" prefWidth="79.0" style="-fx-background-color: e8b3b5;"
      text="Cancel">
      <cursor>
        <Cursor fx:constant="HAND"/>
      </cursor>
    </Button>
    <Button fx:id="confirmButton" layoutX="373.0" layoutY="98.0" mnemonicParsing="false"
      onAction="#confirm" prefHeight="25.0" prefWidth="79.0" style="-fx-background-color: #A3BFB2;"
      text="Confirm">
      <cursor>
        <Cursor fx:constant="HAND"/>
      </cursor>
    </Button>
    <TextField fx:id="field" layoutX="32.0" layoutY="53.0" prefHeight="26.0" prefWidth="419.0"/>
    <Label fx:id="label" layoutX="32.0" layoutY="25.0" text="default">
      <font>
        <Font size="16.0"/>
      </font>
    </Label>
    <Text fx:id="emptyInputError" fill="RED" layoutX="32.0" layoutY="103.0" strokeType="OUTSIDE"
      strokeWidth="0.0" text="Field cannot be empty!" visible="false"/>
  </AnchorPane>
</AnchorPane>
